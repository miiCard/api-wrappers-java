<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ImageServlet</servlet-name>
        <servlet-class>com.miicard.consumers.testharness.ImageServlet</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>CardImageServlet</servlet-name>
        <servlet-class>com.miicard.consumers.testharness.CardImageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ImageServlet</servlet-name>
        <url-pattern>/assuranceimage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CardImageServlet</servlet-name>
        <url-pattern>/cardimage</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
